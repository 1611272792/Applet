/* pages/pricewo/pricewo.wxss */

/* 容器 */

.container {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}

/* 头部 */

.top {
  background-color: #87cefa;
  height: 150px;
}

/* 布局 */

.Grid {
  display: flex;
  font-size: 10px;
  margin-top: 20px;
  background-color: #1e90ff;
}

/*1/3平分*/

.Grid-cell {
  flex: 1;
  height: 30px;
  line-height: 30px;
  text-align: left;
}

/* 头像 */

.myphoto {
  width: 60px;
  height: 60px;
}

.myphotos {
  width: 60px;
  height: 60px;
  background-color: #f5f5f5;
  border-radius: 30px;
  overflow: hidden;
  margin-top: 10px;
}

.myphotoss {
  width: 100%;
  display: flex;
  justify-content: center;
}

/* 文字 */

.vtext {
  text-align: center;
  width: 100%;
  height: 30px;
  line-height: 30px;
}

/* 按钮 */

.mini-btn {
  /* width: 50px; */
  height: 20px;
}

/* 个人奖金布局 */

.weuicell {
  display: flex;
  flex-direction: row;
  height: 46px;
  border-bottom: 1px solid #dcdcdc;
  background-color: #ffffff;
}

.bigweuis {
  width: 33%;
  text-align: left;
  height: 46px;
  line-height: 46px;
  color: #000000;
  margin-left: 5px;
}

.bigweui {
  width: 67%;
  display: flex;
  flex-direction: row;
}

/* 字体颜色 */

.bigweuiss {
  color: #ff0000;
}

.smaweui {
  width: 33%;
  height: 46px;
  line-height: 46px;
  text-align: right;
}

.smaweuis {
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
}

/* 交易、提现按钮 */

.weuibtn {
  height: 30px;
  line-height: 30px;
  background-color: #007aff;
  font-size: 12px;
  color: #ffffff;
}

/* 部门 */



.smalitem {
  width: 100%;
  height: 46px;
  line-height: 46px;
  margin-right: 10px;
}

/* 底部 */

.bottom {
  margin-top: 40px;
  margin-bottom: 10px;
}
.tixian{
  float: right;
  height: 30px;
  line-height: 30px;
  background-color: #007aff;
  font-size: 12px;
  color: #ffffff;
  margin-left: 5px;
}
.tiprice{
  color: #ff0000;
}
