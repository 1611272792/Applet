using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SUNPN.BONUS.Model
{
    public class BonusItem
    {
        /// <summary>
        /// 奖金项Id
        /// </summary>
        private string bonusItemId;
        public string BonusItemId
        {
            get { return bonusItemId; }
            set { bonusItemId = value; }
        }

        /// <summary>
        /// 奖金项名称
        /// </summary>
        private string bonusItemName;
        public string BonusItemName
        {
            get { return bonusItemName; }
            set { bonusItemName = value; }
        }

        /// <summary>
        /// 启用状态（0启用，1禁用）
        /// </summary>
        private int isActive;
        public int IsActive
        {
            get { return isActive; }
            set { isActive = value; }
        }

        /// <summary>
        /// 奖金项负责人Id
        /// </summary>
        private string openId;
        public string OpenId
        {
            get { return openId; }
            set { openId = value; }
        }

        /// <summary>
        /// 奖金项公司Id
        /// </summary>
        private string companyId;
        public string CompanyId
        {
            get { return companyId; }
            set { companyId = value; }
        }

        /// <summary>
        /// 奖金项类型（0公司级，1部门级）
        /// </summary>
        private int bItemType;
        public int BItemType
        {
            get { return bItemType; }
            set { bItemType = value; }
        }

        /// <summary>
        /// 有效期限
        /// </summary>
        private int inDate;
        public int InDate
        {
            get { return inDate; }
            set { inDate = value; }
        }

        /// <summary>
        /// 部门Id(0公司级，其他对应部门Id)
        /// </summary>
        private int depId;
        public int DepId
        {
            get { return depId; }
            set { depId = value; }
        }
    }
}
