using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SUNPN.BONUS.Model
{

    public class BonusImpower
    {
        //奖金项授权Id
        private int biId;
        public int BiId
        {
            get { return biId; }
            set { biId = value; }
        }

        //奖金项Id
        private string bonusItemId;
        public string BonusItemId
        {
            get { return bonusItemId; }
            set { bonusItemId = value; }
        }

        //奖金项负责人
        private string bItemUser;
        public string BItemUser
        {
            get { return bItemUser; }
            set { bItemUser = value; }
        }

        //被授权人
        private string bImpowerUser;
        public string BImpowerUser
        {
            get { return bImpowerUser; }
            set { bImpowerUser = value; }
        }

        //授权金额
        private decimal addMoney;
        public decimal AddMoney
        {
            get { return addMoney; }
            set { addMoney = value; }
        }

        //剩余金额
        private decimal remainMoney;
        public decimal RemainMoney
        {
            get { return remainMoney; }
            set { remainMoney = value; }
        }

        //授权日期
        private DateTime impowerDate;
        public DateTime ImpowerDate
        {
            get { return impowerDate; }
            set { impowerDate = value; }
        }

        //结束日期
        private DateTime endDate;
        public DateTime EndDate
        {
            get { return endDate; }
            set { endDate = value; }
        }

       //是否收回    0不收回 1收回
        private int isVaild;
        public int IsVaild
        {
            get { return isVaild; }
            set { isVaild = value; }
        }

        //模式  0本月有效  1长期有效(不能超过该奖金项的日期)
        private int model;
        public int Model
        {
            get { return model; }
            set { model = value; }
        }

        //备注
        private string remake;
        public string Remake
        {
            get { return remake; }
            set { remake = value; }
        }
    }
}
