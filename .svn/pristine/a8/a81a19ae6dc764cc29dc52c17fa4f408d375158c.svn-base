/* 日期 */

.tui-picker-content {
  overflow: hidden;
  margin-bottom: 10px;
}

.mypicker {
  width: 100%;
}

.tui-picker-detail {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 35rpx;
  width: 100%;
}

/* 删除样式 */

.deleteimg {
  position: absolute;
  top: 0px;
  left: 60px;
  border: 1px solid red;
  width: 20px;
  height: 20px;
}

.deleteimg image {
  width: 20px;
  height: 20px;
}

/* 上传图像 */

.weui-uploader__bd {
  display: flex;
  justify-content: center;
}

/* 默认头像 */

.weuiimg {
  width: 77px;
  height: 77px;
}

.imgviw {
  display: flex;
  justify-content: center;
  position: relative;
}

.deleteimgs {
  position: absolute;
  top: 0px;
  left: 190px;
  width: 20px;
  height: 20px;
}

.scrollview {
  height: 60%;
  position: absolute;
  top: 200rpx;
  /* z-index: 999； */
}

.itemview {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* padding: 20rpx 20rpx 20rpx 20rpx; */
  border-bottom: 1px solid #b1b1b1;
  height: 35px;
  line-height: 35px;
  padding-left: 10px;
  background-color: white;
}
